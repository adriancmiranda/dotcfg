{
  "name": "dotcfg",
  "author": "Adrian C. Miranda",
  "description": "Allows you to manage namespace objects with qualified names",
  "version": "1.5.7",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/adriancmiranda/dotcfg",
  "readmeFilename": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/dotcfg.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/dotcfg/issues"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "clean": "rimraf dist/",
    "dev": "run-p watch:*",
    "benchmark": "ava test/*.benchmark.js",
    "prebuild": "run-s clean test benchmark",
    "build": "webpack",
    "watch": "webpack --progress --colors --watch --env dev",
    "watch:test": "npm test -- --watch",
    "test": "ava test/*.spec.js",
    "travis:test": "npm test",
    "travis:benchmark": "npm run benchmark",
    "appveyor:test": "node --max_old_space_size=4096 node_modules\\ava-cli\\cli test/*.spec.js",
    "appveyor:benchmark": "npm run benchmark",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "release": "npm run build && standard-version"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "ava-cli": "^0.0.1",
    "ava-spec": "^1.1.0",
    "benchmark": "^2.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "conventional-commit-types": "^2.1.0",
    "cross-env": "^3.2.4",
    "es3ify-loader": "^0.2.0",
    "git-revision-webpack-plugin": "^2.4.1",
    "husky": "^0.13.2",
    "make-generator-function": "^1.1.0",
    "microtime": "^2.1.2",
    "moment": "^2.18.0",
    "npm-check-updates": "^2.10.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.1.2",
    "pirate-flag": "^0.0.1",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.11.2",
    "webpack": "^2.2.1",
    "xo": "^0.18.0",
    "xo-loader": "^0.7.0"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "keywords": [
    "ns",
    "obj",
    "dot",
    "namespace",
    "object",
    "variables",
    "config",
    "settings",
    "prop",
    "property",
    "path",
    "access",
    "del",
    "delete",
    "get",
    "set",
    "notation"
  ]
}
