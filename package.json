{
  "name": "dotcfg",
  "author": "Adrian C. Miranda",
  "description": "Allows you to manage namespace objects with qualified names",
  "version": "1.5.13",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/adriancmiranda/dotcfg",
  "readmeFilename": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/dotcfg.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/dotcfg/issues"
  },
  "xo": {
    "overrides": [
      {
        "files": "index.js",
        "esnext": false
      },
      {
        "files": "source/**/*.js",
        "esnext": false
      }
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "clean": "rimraf dist/*.min.js*",
    "dev": "run-p watch:*",
    "benchmark": "ava test/*.benchmark.js",
    "prebuild": "run-s lint test benchmark clean",
    "watch": "webpack --progress --colors --watch --env.dev",
    "build": "webpack --env.dev",
    "bump": "npm run build -- --env dev=0 --env.bump --env.uglify --env.compress",
    "watch:test": "npm test -- --watch",
    "test": "ava test/*.spec.js",
    "lint": "xo index.js source/**/*.js",
    "travis:test": "npm test",
    "travis:benchmark": "npm run benchmark",
    "appveyor:test": "node --max_old_space_size=4096 node_modules\\ava-cli\\cli test/*.spec.js",
    "appveyor:benchmark": "npm run benchmark",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "release": "npm run bump && standard-version"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-cli": "^0.0.1",
    "ava-spec": "^1.1.0",
    "benchmark": "^2.1.4",
    "bump-webpack-plugin": "^0.1.0",
    "compression-webpack-plugin": "^0.4.0",
    "conventional-commit-types": "^2.1.0",
    "cross-env": "^5.0.0",
    "es3ify-loader": "^0.2.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.13.4",
    "make-generator-function": "^1.1.0",
    "microtime": "^2.1.5",
    "moment": "^2.18.1",
    "npm-check-updates": "^2.11.3",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "pirate-flag": "^0.0.1",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.6.1",
    "xo": "^0.18.2",
    "xo-loader": "^0.8.0"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "keywords": [
    "ns",
    "obj",
    "dot",
    "namespace",
    "object",
    "variables",
    "config",
    "settings",
    "prop",
    "property",
    "path",
    "access",
    "del",
    "delete",
    "get",
    "set",
    "notation"
  ]
}