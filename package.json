{
  "name": "dotcfg",
  "author": "Adrian C. Miranda",
  "description": "Allows you to manage namespace objects with qualified names",
  "version": "1.7.6",
  "main": "dist/dotcfg.cjs.js",
  "module": "source/index.js",
  "types": "source/index.d.ts",
  "homepage": "https://github.com/adriancmiranda/dotcfg",
  "readmeFilename": "README.md",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "source"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/dotcfg.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/dotcfg/issues"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "scripts": {
    "bin": "rollup -c bin/index.js",
    "uninstall": "rimraf -rf node_modules package-lock.json",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "untest": "rimraf *.log test/e2e/reports test/e2e/screenshots test/unit/coverage",
    "check": "npx npm-check -uEy",
    "analyze:core:iife": "source-map-explorer dist/dotcfg.iife.min.js.map",
    "analyze:core:amd": "source-map-explorer dist/dotcfg.amd.min.js.map",
    "analyze:core:cjs": "source-map-explorer dist/dotcfg.cjs.min.js.map",
    "analyze:core:umd": "source-map-explorer dist/dotcfg.umd.min.js.map",
    "prebuild": "redrun -s lint test unbuild",
    "dev": "cross-env NODE_ENV=test karma start bin/karma/dev.config.js",
    "dev:doc": "documentation serve --watch dotcfg/index.js",
    "watch": "npm run bin -- -w --environment=NODE_ENV:development",
    "build:doc": "documentation build index.js -f md -o docs",
    "build:gzip": "npm run bin -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run bin -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run bin -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build": "redrun -p build:concat build:minify build:gzip",
    "bump": "npm run build -- --env dev=0 --env.bump --env.uglify --env.compress",
    "report:coverage": "npm run test:cover && codecov -f test/coverage.lcov --token=$CODECOV_TOKEN",
    "test:sauce:modern": "karma start bin/karma/sauce.config.js --env modern",
    "test:sauce:mobile": "karma start bin/karma/sauce.config.js --env mobile",
    "test:sauce:legacy": "karma start bin/karma/sauce.config.js --env legacy",
    "test:sauce": "karma start bin/karma/sauce.config.js",
    "test:codeship": "karma start bin/karma/ci.config.js",
    "test:travis": "karma start bin/karma/ci.config.js",
    "test:appveyor": "karma start bin/karma/ci.config.js",
    "test:unit": "cross-env NODE_ENV=test karma start bin/karma/unit.config.js",
    "test:cover": "cross-env NODE_ENV=test karma start bin/karma/cover.config.js",
    "test:fixture": "node bin/fixture",
    "test:speed": "node bin/benchmark",
    "test": "redrun -s lint flow test:unit",
    "lint": "eslint --ext .js index.js source",
    "flow": "flow",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "release": "npm run bump && standard-version"
  },
  "dependencies": {
    "describe-type": "^1.0.0-dev.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "benchmark": "^2.1.4",
    "chokidar": "2.0.3",
    "conventional-commit-types": "^2.2.0",
    "cross-env": "^5.1.4",
    "cross-spawn": "6.0.5",
    "documentation": "^6.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "flow-bin": "^0.69.0",
    "gh-pages": "1.1.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.14.3",
    "jasmine-core": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-fixture": "0.2.6",
    "karma-html2js-preprocessor": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "karma-jasmine-matchers": "3.7.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "1.5.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "1.2.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "microtime": "^2.1.7",
    "moment": "^2.21.0",
    "pirate-flag": "^0.1.0",
    "redrun": "6.0.1",
    "remove-flow-types-loader": "^1.1.0",
    "rimraf": "^2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-filesize": "4.0.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.2.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "1.0.1",
    "rollup-watch": "4.3.1",
    "sinon": "4.5.0",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.3.0",
    "uglify-es": "3.3.9",
    "validate-commit-msg": "2.14.0",
    "webpack": "4.4.1"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "keywords": [
    "dot",
    "namespace",
    "object",
    "notation"
  ]
}
