{
  "name": "dotcfg",
  "author": "Adrian C. Miranda",
  "description": "Allows you to manage namespace objects with qualified names",
  "version": "1.5.5",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/adriancmiranda/dotcfg",
  "readmeFilename": "README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/dotcfg.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/dotcfg/issues"
  },
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "clean": "rimraf dist/",
    "dev": "run-p watch:*",
    "build": "webpack",
    "prebuild": "run-s clean",
    "watch": "webpack --progress --colors --watch --env dev",
    "watch:test": "npm test -- --watch",
    "test": "ava test/*.spec.js",
    "travis:test": "npm test",
    "appveyor:test": "node --max_old_space_size=4096 node_modules\\ava test/*.spec.js",
    "release": "npm run build && standard-version"
  },
  "devDependencies": {
    "ava-cli": "0.0.1",
    "benchmark": "^2.1.3",
    "compression-webpack-plugin": "^0.3.2",
    "cross-env": "^3.2.4",
    "es3ify-loader": "^0.2.0",
    "git-revision-webpack-plugin": "^2.4.1",
    "make-generator-function": "^1.1.0",
    "moment": "^2.17.1",
    "npm-check-updates": "^2.10.3",
    "npm-run-all": "^4.0.2",
    "pirate-flag": "^0.0.1",
    "rimraf": "^2.6.1",
    "standard-version": "^4.0.0",
    "webpack": "^2.2.1",
    "xo": "^0.17.1",
    "xo-loader": "^0.7.0"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "keywords": [
    "ns",
    "obj",
    "dot",
    "namespace",
    "object",
    "variables",
    "config",
    "settings",
    "prop",
    "property",
    "path",
    "access",
    "del",
    "delete",
    "get",
    "set",
    "notation"
  ]
}
