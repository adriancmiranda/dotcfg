{
  "name": "dotcfg",
  "author": "Adrian C. Miranda",
  "description": "Allows you to manage namespace objects with qualified names",
  "version": "1.6.0",
  "main": "dist/dotcfg.cjs.js",
  "module": "source/index.js",
  "types": "source/index.d.ts",
  "homepage": "https://github.com/adriancmiranda/dotcfg",
  "readmeFilename": "README.md",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "source"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/dotcfg.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/dotcfg/issues"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "scripts": {
    "bin": "rollup -c bin/index.js",
    "uninstall": "rimraf node_modules",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "untest": "rimraf *.log test/e2e/reports test/e2e/screenshots test/unit/coverage",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "analyze:core:iife": "source-map-explorer dist/dotcfg.iife.min.js.map",
    "analyze:core:amd": "source-map-explorer dist/dotcfg.amd.min.js.map",
    "analyze:core:cjs": "source-map-explorer dist/dotcfg.cjs.min.js.map",
    "analyze:core:umd": "source-map-explorer dist/dotcfg.umd.min.js.map",
    "benchmark": "ava test/benchmark/*.bench.js",
    "prebuild": "redrun -s lint test benchmark unbuild",
    "dev": "cross-env NODE_ENV=test karma start bin/karma/dev.config.js",
    "dev:doc": "documentation serve --watch dotcfg/index.js",
    "dev:source": "npm run bin -- -w --environment=NODE_ENV:development",
    "dev:test": "npm test -- --watch",
    "build:doc": "documentation build dotcfg/** -f md -o docs/",
    "build:gzip": "npm run bin -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run bin -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run bin -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build": "redrun -p build:concat build:minify build:gzip",
    "bump": "npm run build -- --env dev=0 --env.bump --env.uglify --env.compress",
    "ci:codeship": "karma start bin/karma/ci.config.js",
    "test:unit": "cross-env NODE_ENV=test karma start bin/karma/unit.config.js",
    "test:cover": "cross-env NODE_ENV=test karma start bin/karma/cover.config.js",
    "test:e2e": "npm run build:concat && node bin/nightwatch/server",
    "test:fixture": "node bin/fixture",
    "test:speed": "node bin/benchmark",
    "test": "ava test/unit/*.spec.js",
    "lint": "eslint --ext .js index.js source",
    "flow": "flow",
    "travis:test": "npm test",
    "travis:benchmark": "npm run benchmark",
    "appveyor:test": "node --max_old_space_size=4096 node_modules\\ava-cli\\cli test/*.spec.js",
    "appveyor:benchmark": "npm run benchmark",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "release": "npm run bump && standard-version"
  },
  "dependencies": {
    "describe-type": "0.6.2"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "ava-cli": "^0.0.1",
    "ava-spec": "^1.1.1",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "benchmark": "^2.1.4",
    "browser-sync": "2.18.13",
    "chokidar": "1.7.0",
    "chromedriver": "2.33.2",
    "conventional-commit-types": "^2.2.0",
    "cross-env": "^5.1.1",
    "cross-spawn": "5.1.0",
    "documentation": "^5.3.3",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.60.0",
    "geckodriver": "1.10.0",
    "gh-pages": "1.1.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.14.3",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-fixture": "0.2.6",
    "karma-html2js-preprocessor": "1.1.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jasmine-matchers": "3.7.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-phantomjs-shim": "1.5.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "1.2.0",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.6",
    "make-arrow-function": "1.1.0",
    "make-generator-function": "^1.1.0",
    "microtime": "^2.1.6",
    "moment": "^2.19.3",
    "nightwatch": "0.9.16",
    "nightwatch-helpers": "1.2.0",
    "npm-check-updates": "^2.13.0",
    "nyc": "^11.3.0",
    "pirate-flag": "^0.1.0",
    "redrun": "5.10.0",
    "remove-flow-types-loader": "^1.1.0",
    "rimraf": "^2.6.2",
    "rollup": "0.52.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-buble": "0.18.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-gzip": "1.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1",
    "selenium-server": "3.8.1",
    "sinon": "4.1.2",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.2.0",
    "tosource": "1.0.0",
    "uglify-es": "3.2.0",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.9.1"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "keywords": [
    "dot",
    "namespace",
    "object",
    "notation"
  ]
}
